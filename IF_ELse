import java.util.*;
public class If_else {
public static void main (String[] args) {
	Scanner sc=new Scanner(System.in);
	int x=sc.nextInt();

	 if(x%2==0)
     {
        if(x>20||x<6)
	   System.out.println("Not Weird");
         else 
       System.out.println("Weird");
	}
	 
	else
		System.out.println("Weird");
}
}
